{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-size-adjust"
        ]
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ]
  }
}